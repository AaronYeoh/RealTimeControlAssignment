0002a1 e079      		ldi	r23,9	;init loop counter
0002a2 1f55      d8u_1:	rol	r21		;shift left dividend
0002a3 957a      		dec	r23		;decrement counter
0002a4 f409      		brne	d8u_2		;if done
0002a5 9508      		ret			;    return
